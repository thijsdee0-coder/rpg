  },
  "keywords": [
    "game",
    "political",
    "rpg",
    "web"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "terser": "^5.24.0",
    "javascript-obfuscator": "^4.1.0"
  }
}

